@article{feng_drivers_2015,
	title = {Drivers of the {US} {CO}2 emissions 1997-2013},
	volume = {6},
	copyright = {© 2015 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	url = {http://www.nature.com/ncomms/2015/150721/ncomms8714/full/ncomms8714.html},
	doi = {10.1038/ncomms8714},
	abstract = {Fossil fuel CO2 emissions in the United States decreased by {\textasciitilde}11\% between 2007 and 2013, from 6,023 to 5,377 Mt. This decline has been widely attributed to a shift from the use of coal to natural gas in US electricity production. However, the factors driving the decline have not been quantitatively evaluated; the role of natural gas in the decline therefore remains speculative. Here we analyse the factors affecting US emissions from 1997 to 2013. Before 2007, rising emissions were primarily driven by economic growth. After 2007, decreasing emissions were largely a result of economic recession with changes in fuel mix (for example, substitution of natural gas for coal) playing a comparatively minor role. Energy–climate policies may, therefore, be necessary to lock-in the recent emissions reductions and drive further decarbonization of the energy system as the US economy recovers and grows.},
	language = {en},
	urldate = {2015-07-22},
	journal = {Nature Communications},
	author = {Feng, Kuishuang and Davis, Steven J. and Sun, Laixiang and Hubacek, Klaus},
	month = jul,
	year = {2015},
	keywords = {Climate science, Earth sciences},
	file = {feng_et_al_2015_drivers_of_the_us_co2_emissions_1997-2013.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/NW6GG2CK/feng_et_al_2015_drivers_of_the_us_co2_emissions_1997-2013.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/EVTI3RZS/ncomms8714.html:text/html}
