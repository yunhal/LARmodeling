@article{ryu_improved_2017,
	title = {Improved modeling of cloudy-sky actinic flux using satellite cloud retrievals},
	volume = {44},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/2016GL071892/abstract},
	doi = {10.1002/2016GL071892},
	abstract = {Clouds play a critical role in modulating tropospheric radiation and thus photochemistry. We develop a methodology for calculating the vertical distribution of tropospheric ultraviolet (300–420 nm) actinic fluxes using satellite cloud retrievals and a radiative transfer model. We demonstrate that our approach can accurately reproduce airborne-measured actinic fluxes from the 2013 Studies of Emissions and Atmospheric Composition, Clouds and Climate Coupling by Regional Surveys (SEAC4RS) campaign as a case study. The results show that the actinic flux is reduced below moderately thick clouds with increasing cloud optical depth and can be enhanced by a factor of 2 above clouds. Inside clouds, the actinic flux can be enhanced by up to 2.4 times in the upper part of clouds or reduced up to 10 times in the lower parts of clouds. Our study suggests that the use of satellite-derived actinic fluxes as input to chemistry-transport models can improve the accuracy of photochemistry calculations.},
	language = {en},
	number = {3},
	journal = {Geophysical Research Letters},
	author = {Ryu, Young-Hee and Hodzic, Alma and Descombes, Gael and Hall, Samuel and Minnis, Patrick and Spangenberg, Douglas and Ullmann, Kirk and Madronich, Sasha},
	month = feb,
	year = {2017},
	keywords = {0317 Chemical kinetic and photochemical properties, 0321 Cloud/radiation interaction, 0360 Radiation: transmission and scattering, actinic flux, Clouds, photolysis, radiative transfer model, satellite},
	pages = {2016GL071892},
	file = {ryu_et_al_2017_improved_modeling_of_cloudy-sky_actinic_flux_using_satellite_cloud.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/2789J93M/ryu_et_al_2017_improved_modeling_of_cloudy-sky_actinic_flux_using_satellite_cloud.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/GKWJ4SGB/abstract.html:text/html}
