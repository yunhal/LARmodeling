@article{tu_long-range_2004,
	title = {Long-range transport of sulfur dioxide in the central {Pacific}},
	volume = {109},
	copyright = {Copyright 2004 by the American Geophysical Union.},
	issn = {2156-2202},
	url = {http://onlinelibrary.wiley.com.proxy.lib.duke.edu/doi/10.1029/2003JD004309/abstract},
	doi = {10.1029/2003JD004309},
	abstract = {Long-range transport of sulfur dioxide (SO2) from east Asia to the central North Pacific troposphere was observed on transit flights during the NASA Transport and Chemical Evolution over the Pacific mission. A series of SO2-enhanced layers above the boundary layer was observed during these flights. The significant features included enhanced SO2 layers associated with low water vapor and low turbulence that were usually dynamically isolated from the marine boundary layer. This study shows that atmospheric dynamics were very important in determining the SO2 distributions in the central Pacific during March and April 2001. Trajectory studies revealed that SO2-enhanced layers could be connected to both volcanic and anthropogenic sources in east Asia. These trajectory studies also showed that the air parcels usually were lifted 2 km above the source regions and then progressed to the east in the midlatitudes (30°–60°N). The air parcels arrived in the central Pacific within 2–3 days. Sulfur dioxide transported at altitudes of 2–4 km dominated the SO2 distribution in the central Pacific. A comparison of SO2 observations and results of chemical transport models indicated that SO2 was removed primarily by cloud processes. Therefore, in the absence of cloud, SO2 can be transported long distances if the trajectory is decoupled from the boundary layer. Another important observation was that the Miyake-jima volcano made a major contribution to the SO2 concentrations in the central Pacific troposphere during March and April 2001. At times, the volcanic SO2 had more influence in the central Pacific than the six largest anthropogenic SO2 source regions in east Asia.},
	language = {en},
	number = {D15},
	urldate = {2014-12-08},
	journal = {Journal of Geophysical Research: Atmospheres},
	author = {Tu, Fang Huang and Thornton, Donald C. and Bandy, Alan R. and Carmichael, Gregory R. and Tang, Youhua and Thornhill, K. Lee and Sachse, Glenn W. and Blake, Donald R.},
	year = {2004},
	keywords = {0322 Constituent sources and sinks, 0365 Troposphere: composition and chemistry, 0368 Troposphere: constituent transport and chemistry, 0370 Volcanic effects, long-range transport, North Pacific, sulfur dioxide},
	pages = {D15S08},
	file = {Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/XHPQNQU6/full\;jsessionid=A144E5E39C821470F2D0E939D601D9CF.html:text/html;tu_et_al_2004_long-range_transport_of_sulfur_dioxide_in_the_central_pacific.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/4IU3XWFR/tu_et_al_2004_long-range_transport_of_sulfur_dioxide_in_the_central_pacific.pdf:application/pdf}
}

