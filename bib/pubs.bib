@article{spracklen_wildfires_2007,
	title = {Wildfires drive interannual variability of organic carbon aerosol in the western {U}.{S}. in summer},
	volume = {34},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2007GL030037/abstract},
	doi = {10.1029/2007GL030037},
	abstract = {Forest wildfire area burned in the western U.S. has increased in recent decades resulting in a substantial organic carbon (OC) source with large interannual variability. We derive OC emissions from wildfires using data for area burned for 1980–2004 and ecosystem specific fuel loadings. For the period 1989–2004 we analyze OC observations in the western U.S. from the IMPROVE network and use a global chemical transport model to simulate OC concentrations. Modeled and observed OC concentrations are highly correlated when we use interannually varying fire emissions (R2 = 0.88); the correlation is smaller with climatological emissions (R2 = 0.4). We estimate that the observed increase in wildfire activity after the mid 1980s has caused mean OC concentrations in summer over the western U.S. to increase by 30\% relative to 1970–1984. In the coming decades, climate change will likely cause further increases in wildfires resulting in increased OC concentrations with implications for health and visibility.},
	language = {en},
	number = {16},
	urldate = {2016-08-31},
	journal = {Geophysical Research Letters},
	author = {Spracklen, Dominick V. and Logan, Jennifer A. and Mickley, Loretta J. and Park, Rokjin J. and Yevich, Rosemarie and Westerling, Anthony L. and Jaffe, Dan A.},
	month = aug,
	year = {2007},
	keywords = {0305 Aerosols and particles, 0315 Biosphere/atmosphere interactions, 0365 Troposphere: composition and chemistry, 0426 Biosphere/atmosphere interactions, Aerosols, biomass burning, climate change},
	pages = {L16816},
	file = {Full Text PDF:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/RZA2V98U/Spracklen et al. - 2007 - Wildfires drive interannual variability of organic.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/TC593JB5/abstract.html:text/html}
