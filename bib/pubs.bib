	file = {Atmos. Chem. Phys. Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/5DIABDHD/2013.html:text/html;young_et_al_2013_pre-industrial_to_end_21st_century_projections_of_tropospheric_ozone.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/Q7SZCVKS/young_et_al_2013_pre-industrial_to_end_21st_century_projections_of_tropospheric_ozone.pdf:application/pdf}
