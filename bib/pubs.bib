@article{carslaw_large_2013,
	title = {Large contribution of natural aerosols to uncertainty in indirect forcing},
	volume = {503},
	copyright = {© 2013 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {0028-0836},
	url = {http://www.nature.com/nature/journal/v503/n7474/full/nature12674.html},
	doi = {10.1038/nature12674},
	abstract = {The effect of anthropogenic aerosols on cloud droplet concentrations and radiative properties is the source of one of the largest uncertainties in the radiative forcing of climate over the industrial period. This uncertainty affects our ability to estimate how sensitive the climate is to greenhouse gas emissions. Here we perform a sensitivity analysis on a global model to quantify the uncertainty in cloud radiative forcing over the industrial period caused by uncertainties in aerosol emissions and processes. Our results show that 45 per cent of the variance of aerosol forcing since about 1750 arises from uncertainties in natural emissions of volcanic sulphur dioxide, marine dimethylsulphide, biogenic volatile organic carbon, biomass burning and sea spray. Only 34 per cent of the variance is associated with anthropogenic emissions. The results point to the importance of understanding pristine pre-industrial-like environments, with natural aerosols only, and suggest that improved measurements and evaluation of simulated aerosols in polluted present-day conditions will not necessarily result in commensurate reductions in the uncertainty of forcing estimates.},
	language = {en},
	number = {7474},
	urldate = {2014-10-20},
	journal = {Nature},
	author = {Carslaw, K. S. and Lee, L. A. and Reddington, C. L. and Pringle, K. J. and Rap, A. and Forster, P. M. and Mann, G. W. and Spracklen, D. V. and Woodhouse, M. T. and Regayre, L. A. and Pierce, J. R.},
	month = nov,
	year = {2013},
	keywords = {\_tablet\_modified, Atmospheric science, Climate and Earth system modelling},
	pages = {67--71},
	file = {carslaw_et_al_2013_large_contribution_of_natural_aerosols_to_uncertainty_in_indirect.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/4BN826C6/carslaw_et_al_2013_large_contribution_of_natural_aerosols_to_uncertainty_in_indirect.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/V8NKTTRD/nature12674.html:text/html}
}

@article{thornton_transport_1997,
	title = {Transport of sulfur dioxide from the {Asian} {Pacific} {Rim} to the {North} {Pacific} troposphere},
	volume = {102},
	copyright = {Copyright 1997 by the American Geophysical Union.},
	issn = {2156-2202},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/97JD01818/abstract},
	doi = {10.1029/97JD01818},
	abstract = {The NASA Pacific Exploratory Mission over the Western Pacific Ocean (PEM-West B) field experiment provided an opportunity to study sulfur dioxide (SO2) in the troposphere over the western Pacific Ocean from the tropics to 60°N during February–March 1993. The large suite of chemical and physical measurements yielded a complex matrix in which to understand the distribution of sulfur dioxide over the western Pacific region. In contrast to the late summer period of Pacific Exploratory Mission-West A (PEM-West A) (1991) over this same area, SO2 showed little increase with altitude, and concentrations were much lower in the free troposphere than during the PEM-West B period. Volcanic impacts on the upper troposphere were again found as a result of deep convection in the tropics. Extensive emission of SO2 from the Pacific Rim land masses were primarily observed in the lower well-mixed part of the boundary layer but also in the upper part of the boundary layer. Analyses of the SO2 data with aerosol sulfate, beryllium-7, and lead-210 indicated that SO2 contributed to half or more of the observed total oxidized sulfur (SO2 plus aerosol sulfate) in free tropospheric air. The combined data set suggests that SO2 above 8.5 km is transported from the surface but with aerosol sulfate being removed more effectively than SO2. Cloud processing and rain appeared to be responsible for lower SO2 levels between 3 and 8.5 km than above or below this region.},
	language = {en},
	number = {D23},
	urldate = {2014-12-08},
	journal = {Journal of Geophysical Research: Atmospheres},
	author = {Thornton, Donald C. and Bandy, Alan R. and Blomquist, Byron W. and Talbot, Robert W. and Dibb, Jack E.},
	year = {1997},
	keywords = {0345 Pollution: urban and regional, 0365 Troposphere: composition and chemistry, 0368 Troposphere: constituent transport and chemistry, 9355 Information Related to Geographic Region: Pacific Ocean},
	pages = {28489--28499},
	file = {Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/WBV4T8NZ/abstract.html:text/html;thornton_et_al_1997_transport_of_sulfur_dioxide_from_the_asian_pacific_rim_to_the_north.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/A3BECI8W/thornton_et_al_1997_transport_of_sulfur_dioxide_from_the_asian_pacific_rim_to_the_north.pdf:application/pdf}
}

