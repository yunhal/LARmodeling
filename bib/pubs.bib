@article{wang_factors_2001,
	title = {Factors controlling tropospheric {O}3, {OH}, {NO} x  and {SO}2 over the tropical {Pacific} during {PEM}-{Tropics} {B}},
	volume = {106},
	copyright = {Copyright 2001 by the American Geophysical Union.},
	issn = {2156-2202},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2001JD900084/abstract},
	doi = {10.1029/2001JD900084},
	abstract = {Observations over the tropical Pacific during the Pacific Exploratory Mission (PEM)-Tropics B experiment (March-April 1999) are analyzed. Concentrations of CO and long-lived nonmethane hydrocarbons in the region are significantly enhanced due to transport of pollutants from northern industrial continents. This pollutant import also enhances moderately O3 concentrations but not NOx concentrations. It therefore tends to depress OH concentrations over the tropical Pacific. These effects contrast to the large enhancements of O3 and NOx concentrations and the moderate increase of OH concentrations due to biomass burning outflow during the PEM-Tropics A experiment (September-October 1996). Observed CH3I concentrations, as in PEM-Tropics A, indicate that convective mass outflux in the middle and upper troposphere is largely independent of altitude over the tropical Pacific. Constraining a one-dimensional model with CH3I observations yields a 10-day timescale for convective turnover of the free troposphere, a factor of 2 faster than during PEM-Tropics A. Model simulated HO2, CH2O, H2O2, and CH3OOH concentrations are generally in agreement with observations. However, simulated OH concentrations are lower (∼25\%) than observations above 6 km. Whereas models tend to overestimate previous field measurements, simulated HNO3 concentrations during PEM-Tropics B are too low (a factor of 2–4 below 6 km) compared to observations. Budget analyses indicate that chemical production of O3 accounts for only 50\% of chemical loss; significant transport of O3 into the region appears to take place within the tropics. Convective transport Of CH3OOH enhances the production of HOx and O3 in the upper troposphere, but this effect is offset by HOx loss due to the scavenging of H2O2. Convective transport and scavenging of reactive nitrogen species imply a necessary source of 0.4–1 Tg yr−1 of NOx in the free troposphere (above 4 km) over the tropics. A large fraction of the source could be from marine lightning. Oxidation of DMS transported by convection from the boundary layer could explain the observed free tropospheric SO2 concentrations over the tropical Pacific. This source of DMS due to convection, however, would imply in the model free tropospheric concentrations much higher than observed. The model overestimate cannot be reconciled using recent kinetics measurements of the DMS-OH adduct reaction at low pressures and temperatures and may reflect enhanced OH oxidation of DMS during convection.},
	language = {en},
	number = {D23},
	urldate = {2014-12-08},
	journal = {Journal of Geophysical Research: Atmospheres},
	author = {Wang, Yuhang and Liu, Shaw C. and Wine, Paul H. and Davis, Douglas D. and Sandholm, Scott T. and Atlas, Eillot L. and Avery, Melody A. and Blake, Donald R. and Blake, Nicola J. and Brune, William H. and Heikes, Brian G. and Sachse, Glen W. and Shetter, Richard E. and Singh, Hanwant B. and Talbot, Robert W. and Tan, David},
	year = {2001},
	keywords = {0315 Biosphere/atmosphere interactions, 0322 Constituent sources and sinks, 0365 Troposphere: composition and chemistry, 0368 Troposphere: constituent transport and chemistry},
	pages = {32733--32747},
	file = {Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/2X28GQBN/abstract.html:text/html;wang_et_al_2001_factors_controlling_tropospheric_o3,_oh,_no_x_and_so2_over_the.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/8Z6RPWXM/wang_et_al_2001_factors_controlling_tropospheric_o3,_oh,_no_x_and_so2_over_the.pdf:application/pdf}
}

@article{sciare_interannual_2000,
	title = {Interannual variability of atmospheric dimethylsulfide in the southern {Indian} {Ocean}},
	volume = {105},
	copyright = {Copyright 2000 by the American Geophysical Union.},
	issn = {2156-2202},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2000JD900236/abstract},
	doi = {10.1029/2000JD900236},
	abstract = {Continuous measurements of atmospheric dimethylsulfide (DMS) have been performed over a 10-year period (1990–1999) at Amsterdam Island in the southern Indian Ocean. Atmospheric DMS ranges from 5 to 1930 parts per trillion by volume (pptv) and shows a clear seasonal variation with a factor of 20 in amplitude between its maximum in January (austral summer) and minimum in July-August (austral winter). Important deviations from the 10-year monthly mean as high as 100\% have been detected, which could not be explained by changes in meteorology and/or oxidation capacity of the atmosphere. Comparison with a three dimensional (3-D) chemistry/transport model revealed that changes in the source strength of DMS as high as a factor of 2 were required to account for such DMS interannual variations. In addition, DMS variability was found to be closely related to sea surface temperature anomalies, clearly indicating a link between DMS and climate changes.},
	language = {en},
	number = {D21},
	urldate = {2014-12-05},
	journal = {Journal of Geophysical Research: Atmospheres},
	author = {Sciare, J. and Mihalopoulos, N. and Dentener, F. J.},
	month = nov,
	year = {2000},
	keywords = {0300 Atmospheric Composition and Structure, 0312 Air/sea constituent fluxes, 0320 Cloud physics and chemistry, 0322 Constituent sources and sinks, 0330 Geochemical cycles},
	pages = {26369--26377},
	file = {sciare_et_al_2000_interannual_variability_of_atmospheric_dimethylsulfide_in_the.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/7KRF3AVV/sciare_et_al_2000_interannual_variability_of_atmospheric_dimethylsulfide_in_the.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/3WIKPTD4/abstract.html:text/html}
