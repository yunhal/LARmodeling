	file = {Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/G3BNQWRU/abstract.html:text/html;wesely_et_al_1985_measurements_and_parameterization_of_particulate_sulfur_dry.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/5GI6D3MF/wesely_et_al_1985_measurements_and_parameterization_of_particulate_sulfur_dry.pdf:application/pdf}
