	pages = {917--923},
	file = {Full Text PDF:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/AAB3FJ8Q/Thompson et al. - 2014 - A systems approach to evaluating the air quality c.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/558HVUPH/nclimate2342.html:text/html}
