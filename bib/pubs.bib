@article{boucher_indirect_2009,
	title = {The indirect global warming potential and global temperature change potential due to methane oxidation},
	volume = {4},
	issn = {1748-9326},
	url = {http://iopscience.iop.org/1748-9326/4/4/044007},
	doi = {10.1088/1748-9326/4/4/044007},
	abstract = {Methane is the second most important anthropogenic greenhouse gas in the atmosphere next to carbon dioxide. Its global warming potential (GWP) for a time horizon of 100 years is 25, which makes it an attractive target for climate mitigation policies. Although the methane GWP traditionally includes the methane indirect effects on the concentrations of ozone and stratospheric water vapour, it does not take into account the production of carbon dioxide from methane oxidation. We argue here that this CO2-induced effect should be included for fossil sources of methane, which results in slightly larger GWP values for all time horizons. If the global temperature change potential is used as an alternative climate metric, then the impact of the CO2-induced effect is proportionally much larger. We also discuss what the correction term should be for methane from anthropogenic biogenic sources.},
	language = {en},
	number = {4},
	urldate = {2015-07-27},
	journal = {Environmental Research Letters},
	author = {Boucher, Olivier and Friedlingstein, Pierre and Collins, Bill and Shine, Keith P.},
	month = oct,
	year = {2009},
	pages = {044007},
	file = {boucher_et_al_2009_the_indirect_global_warming_potential_and_global_temperature_change.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/HNSGVD78/boucher_et_al_2009_the_indirect_global_warming_potential_and_global_temperature_change.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/RUZ5EXDE/044007.html:text/html}
}

@article{zhu_impacts_2013,
	title = {Impacts of the {Bermuda} {High} on {Regional} {Climate} and {Ozone} over the {United} {States}},
	volume = {26},
	issn = {0894-8755},
	url = {http://journals.ametsoc.org/doi/full/10.1175/JCLI-D-12-00168.1},
	doi = {10.1175/JCLI-D-12-00168.1},
	abstract = {AbstractObservations reveal that, in summer, westward extension of the Bermuda high enhances the Great Plains low-level jet (LLJ) that transports more moisture northward, causing precipitation increases in the Midwest and decreases in the Gulf States. Meanwhile, more warm air advection from the Gulf of Mexico to the southern Great Plains and stronger clear-sky radiative heating under high pressures over the Southeast result in warmer surface temperatures across the Gulf states. The enhanced LLJ transport of cleaner marine air from the Gulf reduces surface ozone across the southern Great Plains–Midwest. In contrast, larger transport of more polluted air from the Midwest to New England and more frequent air stagnation under high pressures in the Southeast increase ozone over most of the eastern coastal states. This Bermuda high–induced ozone change reversal between the southern Great Plains–Midwest and eastern coastal states, with a magnitude of 6 and 13.5 ppb, respectively, in summer-mean maximum daily 8-h average, exhibits strong decadal variations that should be considered in the U.S. air quality dynamic management.The observed Bermuda high signatures over the Gulf states can be well captured by regional climate and air quality models. Notable model deficiencies exist over the northern Great Plains–Midwest that are more remote to the Bermuda high and LLJ control. The regional models largely reduce these deficiencies from general circulation models (GCMs). Only 7 out of 51 GCMs can represent all key regional signatures of the Bermuda high, while none can simulate its strong association with planetary sea surface temperature anomalies. The result indicates a great challenge for GCMs to predict Bermuda high variability and change.},
	number = {3},
	urldate = {2015-01-05},
	journal = {Journal of Climate},
	author = {Zhu, Jinhong and Liang, Xin-Zhong},
	month = feb,
	year = {2013},
	keywords = {Air quality, Decadal variability, General circulation models, Interannual variability, Model evaluation/performance, Regional models},
	pages = {1018--1032},
	file = {Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/75RCBDHN/JCLI-D-12-00168.html:text/html;zhu_et_al_2013_impacts_of_the_bermuda_high_on_regional_climate_and_ozone_over_the.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/TAKD8IIA/zhu_et_al_2013_impacts_of_the_bermuda_high_on_regional_climate_and_ozone_over_the.pdf:application/pdf}
}

