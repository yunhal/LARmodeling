	author = {Van Dingenen, Rita and Raes, Frank and Putaud, Jean-P and Baltensperger, Urs and Charron, Aurélie and Facchini, M. -Cristina and Decesari, Stefano and Fuzzi, Sandro and Gehrig, Robert and Hansson, Hans-C and Harrison, Roy M and Hüglin, Cristoph and Jones, Alan M and Laj, Paolo and Lorbeer, Gundi and Maenhaut, Willy and Palmgren, Finn and Querol, Xavier and Rodriguez, Sergio and Schneider, Jürgen and Brink, Harry ten and Tunved, Peter and Tørseth, Kjetil and Wehner, Birgit and Weingartner, Ernest and Wiedensohler, Alfred and Wåhlin, Peter},
	month = may,
