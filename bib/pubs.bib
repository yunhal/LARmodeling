	number = {6345},
	urldate = {2017-07-02},
	journal = {Science},
	author = {Hsiang, Solomon and Kopp, Robert and Jina, Amir and Rising, James and Delgado, Michael and Mohan, Shashank and Rasmussen, D. J. and Muir-Wood, Robert and Wilson, Paul and Oppenheimer, Michael and Larsen, Kate and Houser, Trevor},
	month = jun,
	year = {2017},
	pmid = {28663496},
	pages = {1362--1369},
	file = {hsiang_et_al_2017_estimating_economic_damage_from_climate_change_in_the_united_states.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/S96MFA8M/hsiang_et_al_2017_estimating_economic_damage_from_climate_change_in_the_united_states.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/RZ3DC4Z8/1362.html:text/html}
