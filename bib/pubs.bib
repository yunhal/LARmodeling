@article{marlon_wildfire_2009,
	title = {Wildfire responses to abrupt climate change in {North} {America}},
	volume = {106},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/106/8/2519},
	doi = {10.1073/pnas.0808212106},
	abstract = {It is widely accepted, based on data from the last few decades and on model simulations, that anthropogenic climate change will cause increased fire activity. However, less attention has been paid to the relationship between abrupt climate changes and heightened fire activity in the paleorecord. We use 35 charcoal and pollen records to assess how fire regimes in North America changed during the last glacial–interglacial transition (15 to 10 ka), a time of large and rapid climate changes. We also test the hypothesis that a comet impact initiated continental-scale wildfires at 12.9 ka; the data do not support this idea, nor are continent-wide fires indicated at any time during deglaciation. There are, however, clear links between large climate changes and fire activity. Biomass burning gradually increased from the glacial period to the beginning of the Younger Dryas. Although there are changes in biomass burning during the Younger Dryas, there is no systematic trend. There is a further increase in biomass burning after the Younger Dryas. Intervals of rapid climate change at 13.9, 13.2, and 11.7 ka are marked by large increases in fire activity. The timing of changes in fire is not coincident with changes in human population density or the timing of the extinction of the megafauna. Although these factors could have contributed to fire-regime changes at individual sites or at specific times, the charcoal data indicate an important role for climate, and particularly rapid climate change, in determining broad-scale levels of fire activity.},
	language = {en},
	number = {8},
	urldate = {2016-08-17},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Marlon, J. R. and Bartlein, P. J. and Walsh, M. K. and Harrison, S. P. and Brown, K. J. and Edwards, M. E. and Higuera, P. E. and Power, M. J. and Anderson, R. S. and Briles, C. and Brunelle, A. and Carcaillet, C. and Daniels, M. and Hu, F. S. and Lavoie, M. and Long, C. and Minckley, T. and Richard, P. J. H. and Scott, A. C. and Shafer, D. S. and Tinner, W. and Umbanhowar, C. E. and Whitlock, C.},
	month = feb,
	year = {2009},
	pmid = {19190185},
	keywords = {biomass burning, charcoal, comet, Younger Dryas},
	pages = {2519--2524},
	file = {Marlon et al. - 2009 - Wildfire responses to abrupt climate change in Nor.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/MBPWKCXT/Marlon et al. - 2009 - Wildfire responses to abrupt climate change in Nor.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/9HCFTUC6/2519.html:text/html}
}

@article{binkowski_models-3_2003,
	title = {Models-3 {Community} {Multiscale} {Air} {Quality} ({CMAQ}) model aerosol component 1. {Model} description},
	volume = {108},
	issn = {2156-2202},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2001JD001409/abstract},
	doi = {10.1029/2001JD001409},
	abstract = {The aerosol component of the Community Multiscale Air Quality (CMAQ) model is designed to be an efficient and economical depiction of aerosol dynamics in the atmosphere. The approach taken represents the particle size distribution as the superposition of three lognormal subdistributions, called modes. The processes of coagulation, particle growth by the addition of mass, and new particle formation, are included. Time stepping is done with analytical solutions to the differential equations for the conservation of number, surface area, and species mass. The component considers both PM2.5 and PM10 and includes estimates of the primary emissions of elemental and organic carbon, dust, and other species not further specified. Secondary species considered are sulfate, nitrate, ammonium, water, and secondary organics from precursors of anthropogenic and biogenic origin. Extinction of visible light by aerosols is represented by two methods: a parametric approximation to Mie extinction and an empirical approach based upon field data. The algorithms that simulate cloud interactions with aerosols are also described. Results from box model and three-dimensional simulations are exhibited.},
	language = {en},
	number = {D6},
	urldate = {2016-08-17},
	journal = {Journal of Geophysical Research: Atmospheres},
	author = {Binkowski, Francis S. and Roselle, Shawn J.},
	month = mar,
	year = {2003},
	keywords = {\_tablet, 0305 Aerosols and particles, 0345 Pollution: urban and regional, 0365 Troposphere: composition and chemistry, 0368 Troposphere: constituent transport and chemistry, aerosol species, air quality modeling, Models-3/CMAQ, PM, visibility},
	pages = {4183},
	file = {binkowski_et_al_2003_models-3_community_multiscale_air_quality_(cmaq)_model_aerosol.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/F2XJCTDN/binkowski_et_al_2003_models-3_community_multiscale_air_quality_(cmaq)_model_aerosol.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/MGNEJVJ7/abstract.html:text/html}
