	pages = {7997--8018},
	file = {Atmos. Chem. Phys. Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/XSJBSTWW/2013.html:text/html;lamarque_et_al_2013_multi-model_mean_nitrogen_and_sulfur_deposition_from_the_atmospheric.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/B67VU2ZH/lamarque_et_al_2013_multi-model_mean_nitrogen_and_sulfur_deposition_from_the_atmospheric.pdf:application/pdf}
}

