@article{kunkel_probable_2013,
	title = {Probable maximum precipitation and climate change},
	volume = {40},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/grl.50334/abstract},
	doi = {10.1002/grl.50334},
	abstract = {Probable maximum precipitation (PMP) is the greatest accumulation of precipitation for a given duration meteorologically possible for an area. Climate change effects on PMP are analyzed, in particular, maximization of moisture and persistent upward motion, using both climate model simulations and conceptual models of relevant meteorological systems. Climate model simulations indicate a substantial future increase in mean and maximum water vapor concentrations. For the RCP8.5 scenario, the changes in maximum values for the continental United States are approximately 20\%–30\% by 2071–2100. The magnitudes of the maximum water vapor changes follow temperature changes with an approximate Clausius-Clapeyron relationship. Model-simulated changes in maximum vertical and horizontal winds are too small to offset water vapor changes. Thus, our conclusion is that the most scientifically sound projection is that PMP values will increase in the future due to higher levels of atmospheric moisture content and consequent higher levels of moisture transport into storms.},
	language = {en},
	number = {7},
	urldate = {2016-04-12},
	journal = {Geophysical Research Letters},
	author = {Kunkel, Kenneth E. and Karl, Thomas R. and Easterling, David R. and Redmond, Kelly and Young, John and Yin, Xungang and Hennon, Paula},
	month = apr,
	year = {2013},
	keywords = {1610 Atmosphere, 1626 Global climate models, 1655 Water cycles, 1817 Extreme events, 1840 Hydrometeorology, climate change, Probable Maximum Precipitation},
	pages = {1402--1408},
	file = {Full Text PDF:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/CNUHB7CW/Kunkel et al. - 2013 - Probable maximum precipitation and climate change.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/F8N9ZVZM/abstract.html:text/html}
}

