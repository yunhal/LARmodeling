@article{mccoy_global_2017,
	title = {The global aerosol-cloud first indirect effect estimated using {MODIS}, {MERRA}, and {AeroCom}},
	issn = {2169-8996},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/2016JD026141/abstract},
	doi = {10.1002/2016JD026141},
	abstract = {Aerosol-cloud interactions (ACI) represent a significant source of forcing uncertainty in global climate models (GCMs). Estimates of radiative forcing due to ACI in Fifth Assessment Report range from −0.5 to −2.5 W m−2. A portion of this uncertainty is related to the first indirect, or Twomey, effect whereby aerosols act as nuclei for cloud droplets to condense upon. At constant liquid water content this increases the number of cloud droplets (Nd) and thus increases the cloud albedo. In this study we use remote-sensing estimates of Nd within stratocumulus regions in combination with state-of-the-art aerosol reanalysis from Modern-Era Retrospective Analysis for Research and Applications version 2 (MERRA2) to diagnose how aerosols affect Nd. As in previous studies, Nd is related to sulfate mass through a power law relationship. The slope of the log-log relationship between Nd and SO4 in maritime stratocumulus is found to be 0.31, which is similar to the range of 0.2–0.8 from previous in situ studies and remote-sensing studies in the pristine Southern Ocean. Using preindustrial emissions models, the change in Nd between preindustrial and present day is estimated. Nd is inferred to have more than tripled in some regions. Cloud properties from Moderate Resolution Imaging Spectroradiometer (MODIS) are used to estimate the radiative forcing due to this change in Nd. The Twomey effect operating in isolation is estimated to create a radiative forcing of −0.97 ± 0.23 W m−2 relative to the preindustrial era.},
	language = {en},
	urldate = {2017-02-18},
	journal = {Journal of Geophysical Research: Atmospheres},
	author = {McCoy, D. T. and Bender, F. a.-M. and Mohrmann, J. K. C. and Hartmann, D. L. and Wood, R. and Grosvenor, D. P.},
	month = jan,
	year = {2017},
	keywords = {0305 Aerosols and particles, 0320 Cloud physics and chemistry, 0345 Pollution: urban and regional, 1610 Atmosphere, 1640 Remote sensing, Aerosol, climate, cloud, microphysics},
	pages = {2016JD026141},
	file = {Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/HII2EFDP/abstract.html:text/html}
}

