@article{shimadera_sensitivity_2014,
	title = {Sensitivity analyses of factors influencing {CMAQ} performance for fine particulate nitrate},
	volume = {64},
	issn = {1096-2247},
	url = {http://dx.doi.org/10.1080/10962247.2013.778919},
	doi = {10.1080/10962247.2013.778919},
	abstract = {Improvement of air quality models is required so that they can be utilized to design effective control strategies for fine particulate matter (PM2.5). The Community Multiscale Air Quality modeling system was applied to the Greater Tokyo Area of Japan in winter 2010 and summer 2011. The model results were compared with observed concentrations of PM2.5 sulfate (SO42-), nitrate (NO3−) and ammonium, and gaseous nitric acid (HNO3) and ammonia (NH3). The model approximately reproduced PM2.5 SO42− concentration, but clearly overestimated PM2.5 NO3− concentration, which was attributed to overestimation of production of ammonium nitrate (NH4NO3). This study conducted sensitivity analyses of factors associated with the model performance for PM2.5 NO3− concentration, including temperature and relative humidity, emission of nitrogen oxides, seasonal variation of NH3 emission, HNO3 and NH3 dry deposition velocities, and heterogeneous reaction probability of dinitrogen pentoxide. Change in NH3 emission directly affected NH3 concentration, and substantially affected NH4NO3 concentration. Higher dry deposition velocities of HNO3 and NH3 led to substantial reductions of concentrations of the gaseous species and NH4NO3. Because uncertainties in NH3 emission and dry deposition processes are probably large, these processes may be key factors for improvement of the model performance for PM2.5 NO3−. Implications: The Community Multiscale Air Quality modeling system clearly overestimated the concentration of fine particulate nitrate in the Greater Tokyo Area of Japan, which was attributed to overestimation of production of ammonium nitrate. Sensitivity analyses were conducted for factors associated with the model performance for nitrate. Ammonia emission and dry deposition of nitric acid and ammonia may be key factors for improvement of the model performance.},
	number = {4},
	urldate = {2017-02-20},
	journal = {Journal of the Air \& Waste Management Association},
	author = {Shimadera, Hikari and Hayami, Hiroshi and Chatani, Satoru and Morino, Yu and Mori, Yasuaki and Morikawa, Tazuko and Yamaji, Kazuyo and Ohara, Toshimasa},
	month = apr,
	year = {2014},
	pages = {374--387},
	file = {shimadera_et_al_2014_sensitivity_analyses_of_factors_influencing_cmaq_performance_for_fine.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/HFB3CDZZ/shimadera_et_al_2014_sensitivity_analyses_of_factors_influencing_cmaq_performance_for_fine.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/MPWMDTKE/10962247.2013.html:text/html}
