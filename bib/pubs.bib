@article{haywood_asymmetric_2013,
	title = {Asymmetric forcing from stratospheric aerosols impacts {Sahelian} rainfall},
	volume = {3},
	copyright = {Copyright Nature Publishing Group Jul 2013},
	issn = {1758678X},
	url = {http://search.proquest.com.proxy.lib.duke.edu/docview/1372335248/abstract?accountid=10598},
	doi = {http://dx.doi.org.proxy.lib.duke.edu/10.1038/nclimate1857},
	abstract = {The Sahelian drought of the 1970s-1990s was one of the largest humanitarian disasters of the past 50 years, causing up to 250,000 deaths and creating 10 million refugees. It has been attributed to natural variability, over-grazing and the impact of industrial emissions of sulphur dioxide. Each mechanism can influence the Atlantic sea surface temperature gradient, which is strongly coupled to Sahelian precipitation. We suggest that sporadic volcanic eruptions in the Northern Hemisphere also strongly influence this gradient and cause Sahelian drought. Using de-trended observations from 1900 to 2010, we show that three of the four driest Sahelian summers were preceded by substantial Northern Hemisphere volcanic eruptions. We use a state-of-the-art coupled global atmosphere-ocean model to simulate both episodic volcanic eruptions and geoengineering by continuous deliberate injection into the stratosphere. In either case, large asymmetric stratospheric aerosol loadings concentrated in the Northern Hemisphere are a harbinger of Sahelian drought whereas those concentrated in the Southern Hemisphere induce a greening of the Sahel. Further studies of the detailed regional impacts on the Sahel and other vulnerable areas are required to inform policymakers in developing careful consensual global governance before any practical solar radiation management geoengineering scheme is implemented.},
	language = {English},
	number = {7},
	urldate = {2015-01-09},
	journal = {Nature Climate Change},
	author = {Haywood, Jim M. and Jones, Andy and Bellouin, Nicolas and Stephenson, David},
	month = jul,
	year = {2013},
	keywords = {Meteorology},
	pages = {660--665},
	file = {haywood_et_al_2013_asymmetric_forcing_from_stratospheric_aerosols_impacts_sahelian.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/RRWNRCGU/haywood_et_al_2013_asymmetric_forcing_from_stratospheric_aerosols_impacts_sahelian.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/97XHPAUD/abstract.html:text/html}
}

