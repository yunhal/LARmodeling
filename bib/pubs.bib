	keywords = {0305 Aerosols and particles, 0322 Constituent sources and sinks, 0345 Pollution: urban and regional, 0365 Troposphere: composition and chemistry, Air quality, Emission factor, Fine particulate matter, Wildfire emission},
	pages = {2016JD026315},
