@article{zaveri_model_2008,
	title = {Model for {Simulating} {Aerosol} {Interactions} and {Chemistry} ({MOSAIC})},
	volume = {113},
	issn = {2156-2202},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2007JD008782/abstract},
	doi = {10.1029/2007JD008782},
	abstract = {This paper describes and evaluates a new Model for Simulating Aerosol Interactions and Chemistry (MOSAIC), with a special focus on addressing the long-standing issues in solving the dynamic partitioning of semivolatile inorganic gases (HNO3, HCl, and NH3) to size-distributed atmospheric aerosol particles. The coupled ordinary differential equations (ODE) for dynamic gas-particle mass transfer are extremely stiff, and the available numerical techniques are either very expensive or produce oscillatory solutions. These limitations are overcome in MOSAIC with a new dynamic gas-particle partitioning module, which is coupled to an efficient and accurate thermodynamics module. The algorithm includes a new concept of “dynamic pH,” a novel formulation for mass transfer to mixed-phase and solid particles, and an adaptive time stepping scheme, which together hold the key to smooth, accurate, and efficient solutions of gas-particle partitioning over the entire relative humidity range. MOSAIC is found to be in excellent agreement with a benchmark version of the model that uses a rigorous solver for integrating the stiff ODEs. The steady-state MOSAIC results for monodisperse aerosol test cases are also in excellent agreement with those obtained with the benchmark equilibrium model AIM. Moreover, the CPU times required for fully dynamic solutions by MOSAIC per size bin per 5 min intervals (typical 3-D model time steps) are similar to those for bulk equilibrium solutions by the computationally efficient but relatively less accurate model ISORROPIA. These results show that MOSAIC is extremely efficient without compromising accuracy, and is therefore highly attractive for use in air quality and regional/global aerosol models.},
	language = {en},
	number = {D13},
	urldate = {2016-01-12},
	journal = {Journal of Geophysical Research: Atmospheres},
	author = {Zaveri, Rahul A. and Easter, Richard C. and Fast, Jerome D. and Peters, Leonard K.},
	month = jul,
	year = {2008},
	keywords = {0305 Aerosols and particles, 0317 Chemical kinetic and photochemical properties, 0345 Pollution: urban and regional, 0355 Thermosphere: composition and chemistry, 0550 Model verification and validation, Aerosol and air quality modeling, aerosol thermodynamics, computational efficiency, gas-particle partitioning, numerical methods, stiff ordinary differential equations},
	pages = {D13204},
	file = {Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/GTG5J2AH/abstract.html:text/html;zaveri_et_al_2008_model_for_simulating_aerosol_interactions_and_chemistry_(mosaic).pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/9WJ27QB8/zaveri_et_al_2008_model_for_simulating_aerosol_interactions_and_chemistry_(mosaic).pdf:application/pdf}
}

@article{kloster_dms_2006,
	title = {{DMS} cycle in the marine ocean-atmosphere system – a global model study},
	volume = {3},
	issn = {1726-4189},
	url = {http://www.biogeosciences.net/3/29/2006/},
	doi = {10.5194/bg-3-29-2006},
	abstract = {A global coupled ocean-atmosphere modeling system is established to study the production of dimethylsulfide (DMS) in the ocean, the DMS flux to the atmosphere, and the resulting sulfur concentrations in the atmosphere. The DMS production and consumption processes in the ocean are simulated in the marine biogeochemistry model HAMOCC5, embedded in a ocean general circulation model (MPI-OM). The atmospheric model ECHAM5 is extended by the microphysical aerosol model HAM, treating the sulfur chemistry in the atmosphere and the evolution of the microphysically interacting internally- and externally mixed aerosol populations.
 We simulate a global annual mean DMS sea surface concentration of 1.8 nmol l−1, a DMS emission of 28 Tg(S) yr−1, a DMS burden in the atmosphere of 0.077 Tg(S), and a DMS lifetime of 1.0 days. To quantify the role of DMS in the atmospheric sulfur cycle we simulate the relative contribution of DMS-derived SO2 and SO42− to the total atmospheric sulfur concentrations. DMS contributes 25\% to the global annually averaged SO2 column burden. For SO42− the contribution is 27\%.
 The coupled model setup allows the evaluation of the simulated DMS quantities with measurements taken in the ocean and in the atmosphere. The simulated global distribution of DMS sea surface concentrations compares reasonably well with measurements. The comparison to SO42− surface concentration measurements in regions with a high DMS contribution to SO42− shows an overestimation by the model. This overestimation is most pronounced in the biologically active season with high DMS emissions and most likely caused by a too high simulated SO42− yield from DMS oxidation.},
	number = {1},
	urldate = {2014-12-02},
	journal = {Biogeosciences},
	author = {Kloster, S. and Feichter, J. and Maier-Reimer, E. and Six, K. D. and Stier, P. and Wetzel, P.},
	month = jan,
	year = {2006},
	pages = {29--51},
	file = {Biogeosciences Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/DJ5J6FWX/2006.html:text/html;Biogeosciences Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/TETHNSQ8/2006.html:text/html;kloster_et_al_2006_dms_cycle_in_the_marine_ocean-atmosphere_system_–_a_global_model_study.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/66C43VQQ/kloster_et_al_2006_dms_cycle_in_the_marine_ocean-atmosphere_system_–_a_global_model_study.pdf:application/pdf}
}

