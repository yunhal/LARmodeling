@article{chen_varying_2014,
	title = {Varying planetary heat sink led to global-warming slowdown and acceleration},
	volume = {345},
	issn = {0036-8075, 1095-9203},
	url = {http://www.sciencemag.org.proxy.lib.duke.edu/content/345/6199/897},
	doi = {10.1126/science.1254937},
	abstract = {A vacillating global heat sink at intermediate ocean depths is associated with different climate regimes of surface warming under anthropogenic forcing: The latter part of the 20th century saw rapid global warming as more heat stayed near the surface. In the 21st century, surface warming slowed as more heat moved into deeper oceans. In situ and reanalyzed data are used to trace the pathways of ocean heat uptake. In addition to the shallow La Niña–like patterns in the Pacific that were the previous focus, we found that the slowdown is mainly caused by heat transported to deeper layers in the Atlantic and the Southern oceans, initiated by a recurrent salinity anomaly in the subpolar North Atlantic. Cooling periods associated with the latter deeper heat-sequestration mechanism historically lasted 20 to 35 years.
Deep-sea warming slows down global warming
Global warming seems to have paused over the past 15 years while the deep ocean takes the heat instead. The thermal capacity of the oceans far exceeds that of the atmosphere, so the oceans can store up to 90\% of the heat buildup caused by increased concentrations of greenhouse gases such as carbon dioxide. Chen and Tung used observational data to trace the pathways of recent ocean heating. They conclude that the deep Atlantic and Southern Oceans, but not the Pacific, have absorbed the excess heat that would otherwise have fueled continued warming.
Science, this issue p. 897},
	language = {en},
	number = {6199},
	urldate = {2015-01-09},
	journal = {Science},
	author = {Chen, Xianyao and Tung, Ka-Kit},
	month = aug,
	year = {2014},
	pmid = {25146282},
	pages = {897--903},
	file = {chen_et_al_2014_varying_planetary_heat_sink_led_to_global-warming_slowdown_and.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/J5QRN8FU/chen_et_al_2014_varying_planetary_heat_sink_led_to_global-warming_slowdown_and.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/GI6TRZ59/897.html:text/html}
