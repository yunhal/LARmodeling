	pages = {1471--1479},
	file = {dick_et_al_2000_estimation_of_water_uptake_by_organic_compounds_in_submicron_aerosols.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/RB7QBRWR/dick_et_al_2000_estimation_of_water_uptake_by_organic_compounds_in_submicron_aerosols.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/5DWZTREF/abstract.html:text/html}
