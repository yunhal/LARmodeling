@article{small_can_2009,
	title = {Can aerosol decrease cloud lifetime?},
	volume = {36},
	copyright = {Copyright 2009 by the American Geophysical Union.},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2009GL038888/abstract},
	doi = {10.1029/2009GL038888},
	abstract = {Cloud responses to changes in aerosol remain a dominant uncertainty in the radiative forcing of climate. Two main constructs related to aerosol effects on clouds have been postulated: (i) the “albedo effect” whereby anthropogenic aerosol results in increased droplet concentrations that generate increases in cloud albedo, all else (particularly cloud water) being equal; (ii) the “lifetime effect” whereby anthropogenic aerosol suppresses precipitation and results in clouds with more liquid water, higher fractional cloudiness, and longer lifetimes. Based on new observations presented here, and supported by previous fine-scale modeling studies, we suggest that the balance of evidence shows that non-precipitating cumulus clouds can experience an evaporation-entrainment feedback, and respond to aerosol perturbations in a manner inconsistent with the traditional “lifetime effect.” Because most cumulus clouds evaporate without producing significant precipitation, this is particularly relevant to estimates of aerosol indirect effects on climate.},
	language = {en},
	number = {16},
	urldate = {2014-12-16},
	journal = {Geophysical Research Letters},
	author = {Small, Jennifer D. and Chuang, Patrick Y. and Feingold, Graham and Jiang, Hongli},
	year = {2009},
	keywords = {0305 Aerosols and particles, 0320 Cloud physics and chemistry, 0345 Pollution: urban and regional, 1610 Atmosphere, aerosol-cloud interactions, cloud lifetime, entrainment},
	pages = {L16806},
	file = {small_et_al_2009_can_aerosol_decrease_cloud_lifetime.pdf:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/HUZC233S/small_et_al_2009_can_aerosol_decrease_cloud_lifetime.pdf:application/pdf;Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/6AT3W2X7/abstract.html:text/html}
}

@article{yu_aerosols_2012,
