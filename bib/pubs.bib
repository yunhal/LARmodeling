@article{morenz_nitrate_2016,
	title = {Nitrate {Photolysis} in {Salty} {Snow}},
	volume = {120},
	issn = {1089-5639},
	url = {http://dx.doi.org/10.1021/acs.jpca.6b06685},
	doi = {10.1021/acs.jpca.6b06685},
	abstract = {Nitrate photolysis from snow can have a significant impact on the oxidative capacity of the local atmosphere, but the factors affecting the release of gas-phase products are not well understood. Here, we report a systematic study of the amounts of NO, NO2, and total nitrogen oxides (NOy) emitted from illuminated snow samples as a function of both nitrate and total salt (NaCl and Instant Ocean) concentration. The results provide experimental evidence that the release of nitrogen oxides to the gas phase is directly related to the expected nitrate concentration in the brine at the surface of the snow crystals. With no added salts, steady-state release of gas-phase products increases to a plateau value with increasing prefreezing nitrate concentration; with the addition of salts, the steady-state gas-phase nitrogen oxides generally decrease with increasing prefreezing NaCl or Instant Ocean concentration. In addition, for these frozen mixed nitrate (25 mM)–salt (0–500 mM) solutions, there is an increase in gas-phase NO2 seen at low added salt amounts, with NO2 production enhanced by up to 42\% at low prefreezing [NaCl] (≤25 mM) and by up to 89\% at prefreezing Instant Ocean concentrations lower than 200 mM [Cl–]. This enhancement may be important to the atmospheric oxidative capacity in polar regions.},
	number = {40},
	urldate = {2016-10-17},
	journal = {The Journal of Physical Chemistry A},
	author = {Morenz, Karen J. and Shi, Qianwen and Murphy, Jennifer G. and Donaldson, D. James},
	month = oct,
	year = {2016},
	pages = {7902--7908},
	file = {ACS Full Text PDF w/ Links:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/ZHS5QQQ7/Morenz et al. - 2016 - Nitrate Photolysis in Salty Snow.pdf:application/pdf;ACS Full Text Snapshot:/Users/yunhalee/Library/Application Support/Zotero/Profiles/4i91u6yd.default/zotero/storage/B4JP792H/acs.jpca.html:text/html}
}

